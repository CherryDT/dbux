    "chai": "3.5.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.2.2",
    "eslint-config-kentcdodds": "^9.0.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "ghooks": "1.3.2",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "0.9.0",
    "inline-manifest-webpack-plugin": "3.0.1",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.1.1",
    "karma-webpack": "1.7.0",
    "mocha": "3.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "2.3.0",
    "offline-plugin": "3.4.2",
    "opt-cli": "1.5.1",
