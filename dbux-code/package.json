{
	"name": "dbux-code",
	"displayName": "dbux-code",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dbuxContextView",
		"onLanguage:javascript"
	],
	"__main": "./src/index.js",
	"main": "./dist/bundle.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dbux-view-container",
					"title": "Dbux",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"dbux-view-container": [
				{
					"id": "dbuxContextView",
					"name": ""
				},
				{
					"id": "dbuxTraceDetailsView",
					"name": "Trace Details"
				}
			]
		},
		"commands": [
			{
				"command": "dbuxPlayback.play",
				"title": "Start",
				"icon": {
					"light": "resources/light/play.svg",
					"dark": "resources/dark/play.svg"
				}
			},
			{
				"command": "dbuxPlayback.pause",
				"title": "Pause",
				"icon": {
					"light": "resources/light/pause.svg",
					"dark": "resources/dark/pause.svg"
				}
			},
			{
				"command": "dbuxPlayback.previousTrace",
				"title": "Previous Trace",
				"icon": {
					"light": "resources/light/previousTrace.svg",
					"dark": "resources/dark/previousTrace.svg"
				}
			},
			{
				"command": "dbuxPlayback.nextTrace",
				"title": "Next Trace",
				"icon": {
					"light": "resources/light/nextTrace.svg",
					"dark": "resources/dark/nextTrace.svg"
				}
			},
			{
				"command": "dbuxPlayback.previousTraceInContext",
				"title": "Step Over",
				"icon": {
					"light": "resources/light/previousTraceInContext.svg",
					"dark": "resources/dark/previousTraceInContext.svg"
				}
			},
			{
				"command": "dbuxPlayback.nextTraceInContext",
				"title": "Step Back",
				"icon": {
					"light": "resources/light/nextTraceInContext.svg",
					"dark": "resources/dark/nextTraceInContext.svg"
				}
			},
			{
				"command": "dbuxContextView.previous",
				"title": "Previous",
				"icon": {
					"light": "resources/light/previous.svg",
					"dark": "resources/dark/previous.svg"
				}
			},
			{
				"command": "dbuxContextView.next",
				"title": "Next",
				"icon": {
					"light": "resources/light/next.svg",
					"dark": "resources/dark/next.svg"
				}
			},
			{
				"command": "dbuxContextView.clear",
				"title": "Clear",
				"icon": {
					"light": "resources/light/clear.svg",
					"dark": "resources/dark/clear.svg"
				}
			},
			{
				"command": "dbuxContextView.addEntry",
				"title": "Add"
			},
			{
				"command": "dbuxContextView.gotoEntry",
				"title": "Goto",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "dbuxContextView.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "dbuxContextView.itemClick",
				"title": "Clicked"
			},
			{
				"command": "dbuxTraceDetailsView.itemClick",
				"title": "Clicked"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "dbuxPlayback.previousTrace",
					"group": "navigation@0"
				},
				{
					"command": "dbuxPlayback.play",
					"when": "dbuxPlaybackPlaying == false",
					"group": "navigation@1"
				},
				{
					"command": "dbuxPlayback.pause",
					"when": "dbuxPlaybackPlaying == true",
					"group": "navigation@2"
				},
				{
					"command": "dbuxPlayback.nextTrace",
					"group": "navigation@3"
				},
				{
					"command": "dbuxPlayback.previousTraceInContext",
					"group": "navigation@4"
				},
				{
					"command": "dbuxPlayback.nextTraceInContext",
					"group": "navigation@5"
				}
			],
			"view/title": [
				{
					"command": "dbuxContextView.previous",
					"when": "view == dbuxContextView",
					"group": "navigation@0"
				},
				{
					"command": "dbuxContextView.next",
					"when": "view == dbuxContextView",
					"group": "navigation@1"
				},
				{
					"command": "dbuxContextView.clear",
					"when": "view == dbuxContextView",
					"group": "navigation@2"
				},
				{
					"command": "dbuxContextView.addEntry",
					"when": "view == dbuxContextView"
				}
			],
			"view/item/context": [
				{
					"command": "dbuxContextView.gotoEntry",
					"when": "view == dbuxContextView && viewItem == event",
					"group": "inline"
				},
				{
					"command": "dbuxContextView.deleteEntry",
					"when": "view == dbuxContextView && viewItem == event"
				}
			]
		}
	},
	"scripts": {
		"_before-run": "npx link-module-alias",
		"postinstall": "npx link-module-alias",
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "npx webpack --mode production",
		"start": "npm run _before-run && npx webpack --mode development --watch",
		"start-debug": "node --inspect-brk=9233 ./node_modules/webpack/bin/webpack --mode development --watch",
		"build-prod": "npx webpack --mode production",
		"build-dev": "npx webpack --mode development",
		"analyze": "npx webpack --mode development --json | npx webpack-bundle-size-analyzer"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"flow-bin": "^0.116.1",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"socket.io": "^2.3.0"
	},
	"peerDependencies": {
		"dbux-common": "../dbux-common",
		"dbux-data": "../dbux-data",
		"lodash": "*"
	},
  "_moduleAliases": {
		"dbux-common": "../dbux-common",
		"dbux-data": "../dbux-data"
	}
}
