{
	"name": "dbux_extension",
	"displayName": "dbux extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dbuxView",
		"onLanguage:javascript"
	],
	"__main": "./src/index.js",
	"main": "./dist/bundle.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dbux-view-container",
					"title": "",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"dbux-view-container": [
				{
					"id": "dbuxView",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "dbuxView.previous",
				"title": "Previous",
				"icon": {
					"light": "resources/light/previous.svg",
					"dark": "resources/dark/previous.svg"
				}
			},
			{
				"command": "dbuxView.next",
				"title": "Next",
				"icon": {
					"light": "resources/light/next.svg",
					"dark": "resources/dark/next.svg"
				}
			},
			{
				"command": "dbuxView.clear",
				"title": "Clear",
				"icon": {
					"light": "resources/light/clear.svg",
					"dark": "resources/dark/clear.svg"
				}
			},
			{
				"command": "dbuxView.addEntry",
				"title": "Add"
			},
			{
				"command": "dbuxView.gotoEntry",
				"title": "Goto",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "dbuxView.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "dbuxView.itemClick",
				"title": "Clicked"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "dbuxView.previous",
					"when": "view == dbuxView",
					"group": "navigation@0"
				},
				{
					"command": "dbuxView.next",
					"when": "view == dbuxView",
					"group": "navigation@1"
				},
				{
					"command": "dbuxView.clear",
					"when": "view == dbuxView",
					"group": "navigation@2"
				},
				{
					"command": "dbuxView.addEntry",
					"when": "view == dbuxView"
				}
			],
			"view/item/context": [
				{
					"command": "dbuxView.gotoEntry",
					"when": "view == dbuxView && viewItem == event",
					"group": "inline"
				},
				{
					"command": "dbuxView.deleteEntry",
					"when": "view == dbuxView && viewItem == event"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "npx webpack --mode production",
		"start": "npx webpack --mode development --watch",
		"start-debug": "node --inspect-brk=9233 ./node_modules/webpack/bin/webpack --mode development --watch",
		"build-prod": "npx webpack --mode production",
		"build-dev": "npx webpack --mode development",
		"analyze": "npx webpack --mode development --json | npx webpack-bundle-size-analyzer"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.7",
		"@babel/core": "^7.7.7",
		"@babel/node": "^7.7.7",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-proposal-function-bind": "^7.7.4",
		"@babel/plugin-proposal-optional-chaining": "^7.7.5",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-syntax-export-default-from": "^7.7.4",
		"@babel/plugin-syntax-flow": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-flow": "^7.8.3",
		"@babel/register": "^7.7.7",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"babel-loader": "^8.0.6",
		"eslint": "^6.6.0",
		"eslint-config-esnext": "^4.0.0",
		"eslint-plugin-flowtype": "^4.6.0",
		"flow-bin": "^0.116.1",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"core-js": "^3.6.1",
		"dbux-common": "file:../dbux-common",
		"dbux-data": "file:../dbux-data",
		"lodash": "^4.17.15",
		"socket.io": "^2.3.0",
		"uws": "github:mmdevries/uws#2.4.0"
	}
}
