{
	"name": "dbux_extension",
	"displayName": "dbux extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dbuxEvents",
		"onLanguage:javascript"
	],
	"__main": "./src/index.js",
	"main": "./dist/bundle.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dbux-event-logger",
					"title": "dbux Event Logger",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"dbux-event-logger": [
				{
					"id": "dbuxEvents",
					"name": "dbux events"
				}
			]
		},
		"commands": [
			{
				"command": "dbuxEvents.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "dbuxEvents.addEntry",
				"title": "Add"
			},
			{
				"command": "dbuxEvents.editEntry",
				"title": "Edit",
				"icon": {
					"light": "resources/light/edit.svg",
					"dark": "resources/dark/edit.svg"
				}
			},
			{
				"command": "dbuxEvents.testEntry",
				"title": "MTest",
				"icon": {
					"light": "resources/light/edit.svg",
					"dark": "resources/dark/edit.svg"
				}
			},
			{
				"command": "dbuxEvents.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "dbuxExtension.dbux_jump",
				"title": "dbuxJump"
			}
		]
	},
	"menus": {
		"view/title": [
			{
				"command": "dbuxEvents.refreshEntry",
				"when": "view == dbuxEvents",
				"group": "navigation"
			},
			{
				"command": "dbuxEvents.addEntry",
				"when": "view == dbuxEvents"
			}
		],
		"view/item/context": [
			{
				"command": "dbuxEvents.editEntry",
				"when": "view == dbuxEvents && viewItem == event",
				"group": "inline"
			},
			{
				"command": "dbuxEvents.testEntry",
				"when": "view == dbuxEvents && viewItem == event",
				"group": "inline"
			},
			{
				"command": "dbuxEvents.deleteEntry",
				"when": "view == dbuxEvents && viewItem == event"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"start": "webpack --mode development --watch",
		"build-prod": "webpack --mode production",
		"build-dev": "webpack --mode development",
		"analyze": "webpack --mode development --json | npx webpack-bundle-size-analyzer"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.7",
		"@babel/core": "^7.7.7",
		"@babel/node": "^7.7.7",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-proposal-function-bind": "^7.7.4",
		"@babel/plugin-proposal-optional-chaining": "^7.7.5",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-syntax-export-default-from": "^7.7.4",
		"@babel/plugin-syntax-flow": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.7",
		"@babel/register": "^7.7.7",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"babel-loader": "^8.0.6",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"socket.io": "^2.3.0",
		"@babel/runtime": "^7.7.7",
		"core-js": "^3.6.2"
	}
}
