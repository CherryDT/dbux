{
	"name": "dbux-code",
	"displayName": "dbux-code",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dbuxContextView",
		"onLanguage:javascript"
	],
	"__main": "./src/index.js",
	"main": "./dist/bundle.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dbuxViewContainer",
					"title": "Dbux",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"dbuxViewContainer": [
				{
					"id": "dbuxCallStackView",
					"name": "Call Stack"
				},
				{
					"id": "dbuxTraceDetailsView",
					"name": "Trace Details"
				},
				{
					"id": "dbuxContextView",
					"name": "Run Contexts"
				},
				{
					"id": "dbuxEditorTracesView",
					"name": "Traces at Cursor"
				}
			]
		},
		"commands": [
			{
				"command": "dbuxContextView.gotoEntry",
				"title": "Goto",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "dbuxContextView.itemClick",
				"title": "Click"
			},
			{
				"command": "dbuxCallStackView.gotoEntry",
				"title": "Goto",
				"icon": {
					"light": "resources/light/boolean.svg",
					"dark": "resources/dark/boolean.svg"
				}
			},
			{
				"command": "dbuxCallStackView.itemClick",
				"title": "Click"
			},
			{
				"command": "dbuxPlayback.previousTrace",
				"title": "Start",
				"icon": {
					"light": "resources/light/previousTrace.svg",
					"dark": "resources/dark/previousTrace.svg"
				}
			},
			{
				"command": "dbuxPlayback.nextTrace",
				"title": "Pause",
				"icon": {
					"light": "resources/light/nextTrace.svg",
					"dark": "resources/dark/nextTrace.svg"
				}
			},
			{
				"command": "dbuxPlayback.play",
				"title": "Start",
				"icon": {
					"light": "resources/light/play.svg",
					"dark": "resources/dark/play.svg"
				}
			},
			{
				"command": "dbuxPlayback.pause",
				"title": "Pause",
				"icon": {
					"light": "resources/light/pause.svg",
					"dark": "resources/dark/pause.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "dbuxPlayback.previousTrace",
					"group": "navigation@0"
				},
				{
					"command": "dbuxPlayback.nextTrace",
					"group": "navigation@3"
				},
				{
					"command": "dbuxPlayback.play",
					"when": "dbuxPlaybackPlaying == false",
					"group": "navigation@1"
				},
				{
					"command": "dbuxPlayback.pause",
					"when": "dbuxPlaybackPlaying == true",
					"group": "navigation@2"
				}
			],
			"view/item/context": [
				{
					"command": "dbuxContextView.gotoEntry",
					"when": "view == dbuxContextView && viewItem == contextNode",
					"group": "inline"
				},
				{
					"command": "dbuxCallStackView.gotoEntry",
					"when": "view == dbuxCallStackView && viewItem == callStackNode",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"_before-run": "npx link-module-alias",
		"postinstall": "npx link-module-alias",
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "npx webpack --mode production",
		"start": "npm run _before-run && npx webpack --mode development --watch",
		"start-debug": "node --inspect-brk=9233 ./node_modules/webpack/bin/webpack --mode development --watch",
		"build-prod": "npx webpack --mode production",
		"build-dev": "npx webpack --mode development",
		"analyze": "npx webpack --mode development --json | npx webpack-bundle-size-analyzer"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"flow-bin": "^0.116.1",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"moment": "^2.24.0",
		"socket.io": "^2.3.0"
	},
	"peerDependencies": {
		"dbux-common": "../dbux-common",
		"dbux-data": "../dbux-data",
		"lodash": "*"
	},
	"_moduleAliases": {
		"dbux-common": "../dbux-common",
		"dbux-data": "../dbux-data"
	}
}
