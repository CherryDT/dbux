{
  "private": true,
  "name": "dbux-babel-plugin",
  "version": "0.0.1",
  "description": "",
  "author": "Domi",
  "module": "src/index.js",
  "main": "src/index.js",
  "bin": {
    "dbux-instrument-file": "./bin/dbux-instrument-file.js"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf dist.samples",
    "_before-run": "npx link-module-alias",
    "build": "npm run clean && npm run _before-run && webpack --config config/webpack.config.js",
    "test": "node --stack-trace-limit=1000 node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "npm run test -- --watch",
    "__archive_build": "npm run clean && npx babel src -d dist",
    "__archive_prepublish": "npm run clean && npm run build",
    "__postinstall": "npm run add-links",
    "add-links": "npm i --ignore-scripts -S ../dbux-common ../dbux-runtime"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.8.3",
    "@babel/generator": "^7.8.3",
    "@babel/parser": "^7.8.3",
    "@babel/template": "^7.8.3",
    "@babel/traverse": "^7.8.3",
    "@babel/types": "^7.8.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-tester": "^8.0.1"
  },
  "keywords": [
    "dbux",
    "interactive",
    "code",
    "dynamic",
    "analysis",
    "tracing",
    "babel-plugin"
  ],
  "dependencies": {
  },
  "_moduleAliases": {}
}
